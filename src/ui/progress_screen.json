  "loading_exp_bar_percentage":{
    "type":"label",
    "text":"#text",
    "color":[ 0.0, 1.0, 0.0 ],
    "shadow":true,
    "anchor_from":"bottom_right",
    "anchor_to":"bottom_right",
    "offset":[ -30, -6 ],
    "bindings":[
      {
        "binding_name":"#loading_bar_percentage"
      },
      {
        "binding_type":"view",
        "source_property_name":"(100000 - #loading_bar_percentage * 100000)",
        "target_property_name":"#loading_exp_bar_percentage"
      },
      {
        "binding_type":"view",
        "source_property_name":"(10000 * ((#loading_exp_bar_percentage > 10000) + (#loading_exp_bar_percentage > 20000) + (#loading_exp_bar_percentage > 30000) + (#loading_exp_bar_percentage > 40000) + (#loading_exp_bar_percentage > 50000) + (#loading_exp_bar_percentage > 60000) + (#loading_exp_bar_percentage > 70000) + (#loading_exp_bar_percentage > 80000) + (#loading_exp_bar_percentage > 90000)))",
        "target_property_name":"#loading_exp_bar_percentage_1"
      },
      {
        "binding_type":"view",
        "source_property_name":"(#loading_exp_bar_percentage - #loading_exp_bar_percentage_1)",
        "target_property_name":"#loading_exp_bar_percentage1"
      },
      {
        "binding_type":"view",
        "source_property_name":"(1000 * ((#loading_exp_bar_percentage1 > 1000) + (#loading_exp_bar_percentage1 > 2000) + (#loading_exp_bar_percentage1 > 3000) + (#loading_exp_bar_percentage1 > 4000) + (#loading_exp_bar_percentage1 > 5000) + (#loading_exp_bar_percentage1 > 6000) + (#loading_exp_bar_percentage1 > 7000) + (#loading_exp_bar_percentage1 > 8000) + (#loading_exp_bar_percentage1 > 9000)))",
        "target_property_name":"#loading_exp_bar_percentage_2"
      },
      {
        "binding_type":"view",
        "source_property_name":"(#loading_exp_bar_percentage1 - #loading_exp_bar_percentage_2)",
        "target_property_name":"#loading_exp_bar_percentage2"
      },
      {
        "binding_type":"view",
        "source_property_name":"(100 * ((#loading_exp_bar_percentage2 > 100) + (#loading_exp_bar_percentage2 > 200) + (#loading_exp_bar_percentage2 > 300) + (#loading_exp_bar_percentage2 > 400) + (#loading_exp_bar_percentage2 > 500) + (#loading_exp_bar_percentage2 > 600) + (#loading_exp_bar_percentage2 > 700) + (#loading_exp_bar_percentage2 > 800) + (#loading_exp_bar_percentage2 > 900)))",
        "target_property_name":"#loading_exp_bar_percentage_3"
      },
      {
        "binding_type":"view",
        "source_property_name":"(#loading_exp_bar_percentage2 - #loading_exp_bar_percentage_3)",
        "target_property_name":"#loading_exp_bar_percentage3"
      },
      {
        "binding_type":"view",
        "source_property_name":"(10 * ((#loading_exp_bar_percentage3 > 10) + (#loading_exp_bar_percentage3 > 20) + (#loading_exp_bar_percentage3 > 30) + (#loading_exp_bar_percentage3 > 40) + (#loading_exp_bar_percentage3 > 50) + (#loading_exp_bar_percentage3 > 60) + (#loading_exp_bar_percentage3 > 70) + (#loading_exp_bar_percentage3 > 80) + (#loading_exp_bar_percentage3 > 90)))",
        "target_property_name":"#loading_exp_bar_percentage_4"
      },
      {
        "binding_type":"view",
        "source_property_name":"((#loading_exp_bar_percentage_1 + #loading_exp_bar_percentage_2 + #loading_exp_bar_percentage_3 + #loading_exp_bar_percentage_4) / 10 + 1)",
        "target_property_name":"#current_loading_exp_bar_percentage"
      },
      {
        "binding_type":"view",
        "source_property_name":"(('§z' + (#current_loading_exp_bar_percentage / 100) + '.' + (#current_loading_exp_bar_percentage - #current_loading_exp_bar_percentage / 100 * 100) + '.' + '%%') - '.1.%%' - '.0.%%' - '.%%' + '%%')",
        "target_property_name":"#text"
      }
    ]
  }
}